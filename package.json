    "pubminor": "npm version minor && npm publish",
    "pubmajor": "npm version major && npm publish",
    "storybook": "storybook dev",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "demo": "vite"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/paramodel.js"
  },
  "types": "./dist/paramodel.d.ts",
  "devDependencies": {
